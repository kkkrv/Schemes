{
   "$schema":"https://json-schema.org/draft/2020-12/schema",
   "title":"Review",
   "description":"A product product review",
   "type":"object",
   "properties":{
      "_id":{
         "type":"string"
      },
      "name":{
         "type":"string"
      },
      "raiting":{
         "type":"integer"
      },
      "comment":{
         "type":"string"
      },
      "createdAt":{
         "type":"string",
         "format":"date-time"
      },
      "updatedAt":{
         "type":"string",
         "format":"date-time"
      }
   },
   "required":[
      "_id",
      "name",
      "raiting",
      "comment",
      "createdAt"
   ],
   "additionalProperties":false
}